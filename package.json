{
  "name": "ReactStarter",
  "version": "0.0.1",
  "description": "React starter",
  "scripts": {
    "storybook": "start-storybook",
    "build-storybook": "build-storybook -s public",
    "------ Starter Project --------": "",
    "starterProject:server": "json-server --watch ./src/appsDevelop/starterProject/server/db.json --port 3010 --routes ./src/appsDevelop/starterProject/server/routes.json",
    "starterProject:start": "npm start -- --env.APP /appsDevelop/starterProject --env.TRANSLATE en,he,ru",
    "starterProject:build-dev": "webpack --mode production --env.APP /appsDevelop/starterProject",
    "starterProject:build-prod": "webpack --mode production -p --env.APP /appsDevelop/starterProject",
    "------ worker Example ------": "",
    "workerExample:start": "npm start -- --env.APP /appsDevelop/workerExample",
    "------ Docs --------": "",
    "docs": "./node_modules/.bin/esdoc -c .esdoc.json",
    "docs:min": "./node_modules/.bin/esdoc -c .esdoc-min.json",
    "------ Tools --------": "",
    "start": "webpack-dev-server  --hot --host 0.0.0.0 --mode development --open",
    "lint": "./node_modules/.bin/standard",
    "lint-fix": "./node_modules/.bin/standard --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.8.6",
    "@babel/node": "7.8.4",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-export-default-from": "7.8.3",
    "@babel/plugin-transform-runtime": "7.8.3",
    "@babel/preset-env": "7.8.6",
    "@babel/preset-react": "7.8.3",
    "@storybook/addon-actions": "5.3.14",
    "@storybook/addon-docs": "5.3.14",
    "@storybook/addon-knobs": "5.3.14",
    "@storybook/addon-links": "5.3.14",
    "@storybook/addon-storysource": "5.3.14",
    "@storybook/preset-create-react-app": "1.5.2",
    "@storybook/react": "5.3.14",
    "react-syntax-highlighter": "12.2.1",
    "babel-core": "6.26.3",
    "babel-loader": "8.0.6",
    "copy-webpack-plugin": "4.6.0",
    "css-loader": "3.4.2",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-react": "7.18.3",
    "html-loader": "0.5.5",
