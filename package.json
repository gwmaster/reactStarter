{
  "name": "ReactStarter",
  "version": "0.0.1",
  "description": "React starter",
  "scripts": {
    "------ Starter Project --------": "",
    "starterProject:server": "json-server --watch ./src/appsDevelop/starterProject/server/db.json --port 3010 --routes ./src/appsDevelop/starterProject/server/routes.json",
    "starterProject:start": "npm start -- --env.APP /appsDevelop/starterProject",
    "starterProject:build-dev": "webpack --mode production --env.APP /appsDevelop/starterProject",
    "starterProject:build-prod": "webpack --mode production -p --env.APP /appsDevelop/starterProject",
    "------ worker Example ------": "",
    "workerExample:start": "npm start -- --env.APP /appsDevelop/workerExample",
    "------ Docs --------": "",
    "docs": "./node_modules/.bin/esdoc -c .esdoc.json",
    "docs:min": "./node_modules/.bin/esdoc -c .esdoc-min.json",
    "------ Dev Code --------": "",
    "testCode:start": "npm start -- --env.APP /appsDevelop/testCode",
    "testCompressions:start": "npm start -- --env.APP /appsDevelop/testCompressions",
    "Grid:start": "npm start -- --env.APP /appsDevelop/Grid",
    "------ Tools --------": "",
    "start": "webpack-dev-server  --hot --host 0.0.0.0 --mode development --open",
    "lint": "./node_modules/.bin/standard",
    "lint-fix": "./node_modules/.bin/standard --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-eslint": "10.0.3",
    "babel-loader": "7.1.5",
    "babel-plugin-import": "1.11.2",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-decorators-legacy": "1.3.5",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "copy-webpack-plugin": "4.6.0",
    "css-loader": "3.2.1",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-react": "7.18.3",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "json-server": "0.15.1",
    "style-loader": "0.23.1",
    "less": "3.10.3",
    "less-loader": "4.1.0",
    "webpack": "4.41.5",
    "webpack-bundle-analyzer": "3.6.0",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.8.2"
  },
  "dependencies": {
    "antd": "3.26.8",
    "axios": "0.19.2",
    "connected-react-router": "6.6.1",
    "fast-redux-reducer": "0.0.4",
    "react": "16.8.6",
    "react-dom": "16.12.0",
    "react-fps-stats": "0.1.2",
    "react-redux": "7.1.3",
    "react-redux-i18n": "1.9.3",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "redux": "3.7.2",
    "redux-localstorage": "0.4.1",
    "redux-logger": "3.0.6",
    "redux-promise": "0.5.3",
    "redux-saga": "1.1.3",
    "redux-saga-worker": "0.0.9",
    "redux-thunk": "2.3.0"
  }
}
